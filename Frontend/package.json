{
  "name": "hello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "axios": "^1.8.4",
    "lucide-react": "^0.487.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^6.30.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false --silent --verbose=false",
    "report:coverage": "codecov -f ./coverage/lcov.info",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.14",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "codecov": "^3.8.3",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "msw": "^2.8.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__mocks__/",
      "/config/",
      "/context/",
      "/services/",
      "src/index.(js|ts)x?",
      "src/App.(js|ts)x?",
      "src/ConstitutionalArchiveHomepage.test.js",
      "src/AdminDashboard.js",
      "src/components/DocumentPreview.js",
      "src/components/DocumentPreviewModal.js",
      "src/components/Dashboard/FeaturedDocuments.js" 
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "silent": true,
    "verbose": false
  }
}